{
  "env": {
    "es6": true,
    "node": true,
  },
  "parserOptions": {
    sourceType: "module"
  },
  "plugins": [
    "node",
    "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "node/no-unsupported-features": 0,
    "no-var": "error",
    "no-multi-spaces": ["error"],
  }
}
