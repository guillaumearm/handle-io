{
  "name": "handle-io",
  "version": "0.1.0",
  "description": "Wrap side effects, combine them, and make this combination testable",
  "main": "lib/handle-io.js",
  "module": "es/handle-io.js",
  "jsnext:main": "es/handle-io.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "docs"
  ],
  "scripts": {
    "clean": "rimraf lib es dist coverage",
    "build": "npm run build:packages",
    "build:packages": "rollup -c",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:update": "npm run test -- --updateSnapshot",
    "test:bundle": "NODE_ENV=test node -r babel-register -r babel-polyfill __bundle_tests__/index.js",
    "test:bundle:watch": "NODE_ENV=test nodemon -i __bundle_tests__/tmp -r babel-register -r babel-polyfill __bundle_tests__/index.js",
    "test:nsp": "nsp check",
    "test:all": "npm run lint && npm run test && npm run test:bundle",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run test:all && npm run test:nsp && npm run clean && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "create-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "cz": "git-cz",
    "bump": "conventional-bump"
  },
  "keywords": [
    "handle-io",
    "io-handler",
    "handler",
    "generator",
    "side-effects",
    "test",
    "testing-io",
    "testable"
  ],
  "author": {
    "name": "Guillaume ARM",
    "email": "trapcodien@gmail.com",
    "url": "https://github.com/guillaumearm"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumearm/handle-io.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumearm/handle-io/issues"
  },
  "homepage": "https://github.com/guillaumearm/handle-io",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "async-describe": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "commitizen": "^2.9.6",
    "conventional-bump": "^1.0.3",
    "conventional-changelog-cli": "^1.3.13",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.0",
    "expect.js": "^0.3.1",
    "greenkeeper-lockfile": "^1.14.0",
    "jest": "^22.3.0",
    "nodemon": "^1.14.12",
    "nsp": "^3.2.1",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.5.0",
    "rimraf": "^2.6.2",
    "rimraf-promise": "^2.0.0",
    "rollup": "^0.56.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "sinon": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  }
}
