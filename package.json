{
  "name": "handle-io",
  "version": "0.1.0",
  "description": "Wrap side effects, combine them, and make this combination testable",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:update": "npm run test -- --updateSnapshot",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test:all": "npm run clean && npm run lint && npm run test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "create-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "conventional-bump"
  },
  "keywords": [
    "handle-io",
    "io-handler",
    "handler",
    "generator",
    "side-effects",
    "test",
    "testing-io",
    "testable"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumearm/handle-io.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumearm/handle-io/issues"
  },
  "homepage": "https://github.com/guillaumearm/handle-io#readme",
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "conventional-bump": "^1.0.3",
    "conventional-changelog-cli": "^1.3.13",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.17.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.0",
    "jest": "^22.3.0",
    "nodemon": "^1.14.12",
    "rimraf": "^2.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  }
}
